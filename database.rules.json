{
  "rules": {
    "resumes": {
      "$id": {
        ".read": true,
        ".write": "auth != null && (
        !data.exists() || data.child('owner').val() === auth.uid
        )"
      }
    },

    "users": {
      "$uid": {
        ".read": "auth != null && auth.uid === $uid",
        ".write": "auth != null && auth.uid === $uid"
      }
    },

    ".read": true,
    ".write": false
  }
}
