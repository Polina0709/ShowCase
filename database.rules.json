{
  "rules": {
    "users": {
      "$uid": {
        ".read": "$uid === auth.uid",
        ".write": "$uid === auth.uid"
      }
    },
    "resumes": {
      "$resumeId": {
        ".read": "data.child('isPublished').val() === true || data.child('owner').val() === auth.uid",
        ".write": "data.child('owner').val() === auth.uid"
      }
    },
    "analytics": {
      "$resumeId": {
        ".read": true,
        ".write": "auth != null"
      }
    }
  }
}
